	option Username 'username'
	option Password 'xxxxxxxx'
	option Nic 'eth0'
	option DNS '114.114.114.114'
	option Timeout '2'
	option EchoInterval '2'
	option RestartWait '1'
	option MaxFail '0'
	option StartMode '0'
	option DhcpMode '2'
	option DaemonMode '3'
	option ShowNotify '0'
	option dhcpscript 'udhcpc -i eth0'
	option IP '0.0.0.0'
	option Mask '255.255.255.0'
	option Gateway '0.0.0.0'
	option PingHost '0.0.0.0'
	option Version '0.00'
	option DataFile ''
